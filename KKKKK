#include <stdio.h>
#include <stdlib.h>


 int is_prime(int num){
 int i;
 for(i=2;i< num; i++){
    if(num%i == 0) return 0;
 }
return 1;
}

int main(){
    int i;
    printf("numeros primos entre 100 e 1000");
    for(i = 100;i <= 1000;i++){
        if (is_prime(i) == 1) printf("%d\n",i);
    }


return 0;
}

