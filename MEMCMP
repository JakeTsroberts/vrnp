#include <stdio.h>
#include <string.h>

int main ()
{
   char str1[15];
   char str2[15];
   int ret;
   strcpy(str1, "Abcd");
   strcpy(str2, "AbCD");
   ret = memcmp(str1, str2,2);
   if(ret == 0)
   {
      printf("str1 e igual a str2");
   }
   else
   {
      printf("str1 e str2 sao diferentes ");
   }
    printf("\n --> %d",ret);
   return(0);
}
