#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main()
{
 char fonte[20];
 char par = ')';
 char *res;
 printf("Digite tel, com ou sem DDD:");
 scanf("%s",fonte);
 res = strchr(fonte,par);
 if(res == NULL) printf("Telefone: %s.\n",fonte);
 else{
  printf("Telefone: %s",res+1);
 }
 return 0;
}
