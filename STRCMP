#include <stdio.h>
#include <string.h>

int main ()
{
   char str1[15];
   char str2[15];
   int ret;
   strcpy(str1, "Abcd");
   strcpy(str2, "ABCD");
   ret = strcmp(str1, str2);
   if(ret == 0)
   {
      printf("str1 e igual a str2");
   }
   else
   {
      printf("str2 e diferente de str1");
   }

   return(0);
}
